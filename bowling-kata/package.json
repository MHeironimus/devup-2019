{
  "name": "bowling-kata",
  "version": "1.0.0",
  "description": "Package containing a simple bowling kata.",
  "repository": {
    "type": "git",
    "url": "https://heironimus.visualstudio.com/TypeScript%20Research/_git/bowling-kata"
  },
  "main": "build/index.js",
  "scripts": {
    "start": "echo \"Not currently available\" && exit 1",
    "build": "tsc",
    "test": "mocha",
    "test-coverage": "nyc --reporter=html --reporter=text mocha",
    "test-mutation": "stryker run"
  },
  "author": "Matthew Heironimus",
  "license": "ISC",
  "devDependencies": {
    "@stryker-mutator/core": "^2.1.0",
    "@stryker-mutator/html-reporter": "^2.1.0",
    "@stryker-mutator/mocha-framework": "^2.1.0",
    "@stryker-mutator/mocha-runner": "^2.1.0",
    "@stryker-mutator/typescript": "^2.1.0",
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "nyc": {
    "exclude": [
      "test/**"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
